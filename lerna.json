{
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish %s"
    }
  },
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/*.md"
  ],
  "packages": [
    "packages/*",
    "examples/*",
    "website"
  ],
  "useWorkspaces": true,
  "version": "independent"
}

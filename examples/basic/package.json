{
  "private": true,
  "name": "example.admob.basic",
  "displayName": "AdmobBasicExample",
  "version": "1.0.0",
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:cordova": "cordova prepare",
    "prepare:plugin": "cordova plugin add ../../packages/cordova-plugin-admob-suite --nosave --link",
    "dev": "run-s dev:*",
    "dev:del": "del platforms/ios/AdmobBasicExample/Plugins/cordova-plugin-admob-suite",
    "dev:link": "ln -s ../../../../plugins/cordova-plugin-admob-suite/src/ios platforms/ios/AdmobBasicExample/Plugins/cordova-plugin-admob-suite",
    "dev:open": "open platforms/ios/AdmobBasicExample.xcworkspace",
    "resinstall-plugin": "run-s resinstall-plugin:* prepare:plugin dev:del dev:link",
    "resinstall-plugin:remove": "cordova plugin remove cordova-plugin-admob-suite"
  },
  "dependencies": {
    "cordova-android": "^7.1.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "devDependencies": {
    "cordova": "^8.0.0",
    "del-cli": "^1.1.0",
    "npm-run-all": "^4.1.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios"
    ]
  }
}

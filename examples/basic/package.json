{
  "private": true,
  "name": "example.admob.basic",
  "displayName": "AdmobBasicExample",
  "version": "1.0.0",
  "scripts": {
    "prepare": "cordova prepare && cordova plugin add ../../packages/cordova-plugin-admob-suite --nosave --link",
    "dev": "cd platforms/ios/AdmobBasicExample/Plugins && del cordova-plugin-admob-suite && ln -s ../../../../plugins/cordova-plugin-admob-suite/src/ios cordova-plugin-admob-suite && open ../../AdmobBasicExample.xcworkspace"
  },
  "dependencies": {
    "cordova-android": "^7.1.0",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "devDependencies": {
    "cordova": "^8.0.0",
    "del-cli": "^1.1.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
